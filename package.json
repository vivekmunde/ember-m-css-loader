  "description": "Lazy loading CSS using simple Ember Service",
  "keywords": [
    "ember-addon",
    "ember-css-loader",
    "ember-css-lazy-loader",
    "ember-css-load-on-demand"
  ],
  "license": "MIT",
  "author": "Vivek Munde",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
